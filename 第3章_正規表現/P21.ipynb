   "source": [
    "# カテゴリ名を含む行を抽出\n",
    "\n",
    "記事中でカテゴリ名を宣言している行を抽出せよ．"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "['[[Category:イギリス|*]]\\n',\n",
       " '[[Category:英連邦王国|*]]\\n',\n",
       " '[[Category:G8加盟国]]\\n',\n",
       " '[[Category:欧州連合加盟国]]\\n',\n",
       " '[[Category:海洋国家]]\\n',\n",
       " '[[Category:君主国]]\\n',\n",
       " '[[Category:島国|くれいとふりてん]]\\n',\n",
       " '[[Category:1801年に設立された州・地域]]']"
      ]
     },
     "execution_count": 1,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "import re\n",
    "\n",
    "RE_CATEGORY = re.compile(r'^\\[\\[Category:.*\\]\\]$')\n",
    "\n",
    "result = []\n",
    "\n",
    "with open('Inglez.txt') as f:\n",
    "    for line in f:\n",
    "        if RE_CATEGORY.match(line):\n",
    "            result.append(line)\n",
    "\n",
    "result"
   ]
  }
 ],
